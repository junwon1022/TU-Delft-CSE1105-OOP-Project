<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.DropShadow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="163.0" prefWidth="260.0" style="-fx-background-color: #CAF0F8;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.scenes.RenameCheckListItemCtrl">
    <children>
        <TextField fx:id="text" layoutX="12.0" layoutY="65.0" onKeyPressed="#handleKeyPressed" prefHeight="33.0" prefWidth="237.0" promptText="Enter the new title" style="-fx-border-color: gray;" stylesheets="@../../styles.css">
            <font>
                <Font name="Corbel" size="12.0" />
            </font></TextField>
        <Label alignment="CENTER" layoutX="12.0" layoutY="14.0" prefHeight="23.0" prefWidth="237.0" text="Rename the Subtask" textAlignment="CENTER">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
        <Label fx:id="nullText" alignment="CENTER" layoutX="10.0" layoutY="37.0" prefHeight="22.0" prefWidth="240.0" style="-fx-text-fill: red;">
            <font>
                <Font name="Corbel" size="12.0" />
            </font>
        </Label>
        <Button fx:id="renameChecklist" layoutX="179.0" layoutY="112.0" mnemonicParsing="false" onAction="#rename" stylesheets="@../../styles.css" text="Rename" textFill="#FFFFFF">
            <font>
                <Font size="13.0" />
            </font></Button>
        <Button fx:id="cancelChecklist" cancelButton="true" layoutX="14.0" layoutY="112.0" mnemonicParsing="false" onAction="#cancel" prefHeight="25.0" prefWidth="59.0" stylesheets="@../../styles.css" text="Cancel" textFill="#FFFFFF">
            <font>
                <Font size="13.0" />
            </font></Button>
    </children>
   <effect>
      <DropShadow />
   </effect>
</AnchorPane>
