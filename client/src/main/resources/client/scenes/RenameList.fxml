<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="200.0" prefWidth="300.0" style="-fx-background-color: #CAF0F8;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.scenes.RenameListCtrl">
   <children>
      <TextField fx:id="textField" layoutX="32.0" layoutY="67.0" onKeyPressed="#handleKeyPressed" prefHeight="33.0" prefWidth="237.0" promptText="Enter a new title . . ." style="-fx-background-color: #E4F8FC; -fx-prompt-text-fill: #665A5D;">
         <font>
            <Font name="Corbel" size="12.0" />
         </font></TextField>
      <Label layoutX="90.0" layoutY="31.0" text="Rename the list" textAlignment="CENTER" textFill="#231f20">
         <font>
            <Font name="Corbel Bold" size="18.0" />
         </font>
      </Label>
      <Button fx:id="rename" layoutX="225.0" layoutY="160.0" mnemonicParsing="false" onAction="#rename" style="-fx-background-color: #00B4D8;" text="Rename" textFill="WHITE">
         <font>
            <Font size="13.0" />
         </font></Button>
      <Button fx:id="cancel" cancelButton="true" layoutX="150.0" layoutY="160.0" mnemonicParsing="false" onAction="#cancel" prefHeight="25.0" prefWidth="59.0" style="-fx-background-color: #C34042;" text="Cancel" textFill="WHITE">
         <font>
            <Font size="13.0" />
         </font></Button>
      <Label fx:id="nullTitle" alignment="CENTER" contentDisplay="CENTER" layoutX="53.0" layoutY="110.0" prefHeight="33.0" prefWidth="202.0" text="" textAlignment="CENTER" textFill="#8f2e30">
         <font>
            <Font name="Corbel" size="12.0" />
         </font></Label>
   </children>
</AnchorPane>
